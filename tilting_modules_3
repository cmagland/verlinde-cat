G := SL(3,2);

// Define tensor powers

V := GModule(G);
V2 := TensorProduct(V, V);
V3 := TensorProduct(V, V2);
V4 := TensorProduct(V, V3);


// Find highest weight modules
// Playing around this seems incorrect

T1 := V;				// Dimension 2
T2 := IndecomposableSummands(V2)[2];	// Dimension 3
T3 := IndecomposableSummands(V3)[2];	// Dimension 6 (Composition series seems to be wrong)
T4 := IndecomposableSummands(V4)[2];	// Dimension 3 (That seems wrong)
